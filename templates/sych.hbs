<div class="p-4 2xl:hidden font-bold">
        ðŸŒ€ {{project}}
    </div>
        <ul
          class="space-y-2 font-medium"
          id="default-styled-tab"
          data-tabs-toggle="#default-styled-tab-content"
          role="tablist"
        >
        {{ #each titles }}
            <li class="me-2" role="presentation">
                <button
                class="flex items-center p-2 text-gray-900 rounded-lg dark:text-white hover:bg-gray-100 dark:hover:bg-gray-700 group"
                id="{{this.1}}-styled-tab"
                data-tabs-target="#styled-{{this.1}}"
                type="button"
                role="tab"
                aria-controls="{{this.1}}"
                aria-selected="false"
                >
                {{this.0}}
                </button>
            </li>
        {{/each}}

        </ul>
      </div>
    </aside>

    <div id="default-styled-tab-content" class="p-4 sm:ml-64">
        {{#each contents}}
            <div
            class="hidden p-4 rounded-lg"
            id="styled-{{this.1}}"
            role="tabpanel"
            aria-labelledby="{{this.1}}-tab"
        >
            {{{this.0}}}
        </div>
        {{/each}}
    </div>

</div>

    <script type="module">
        {{!-- extension imports --}}
        {{#each extensions}}
            import { render as render{{@key}} } from "{{this.url}}"
        {{/each}}
        

        {{!-- extension renders --}}
        {{#each render_targets}}
            render{{this.0}}(document.getElementById("{{this.1}}"), `{{{this.2}}}`);
        {{/each}}
        

        {{{script_content}}}
    </script>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/flowbite/2.3.0/flowbite.min.js"></script>
</body>

